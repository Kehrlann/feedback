<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">
<suppressions>
    <suppress files=".*" checks="JavadocMethod"/>
    <suppress files=".*" checks="JavadocStyle"/>
    <suppress files=".*" checks="JavadocTagContinuationIndentation"/>
    <suppress files=".*" checks="JavadocType"/>
    <suppress files=".*" checks="JavadocVariable"/>
    <suppress files=".*" checks="NonEmptyAtclauseDescription"/>
    <suppress files=".*" checks="SpringJavadoc"/>
    <suppress files=".*" checks="SpringImportOrderCheck"/>
    <suppress files="test/.*\.java" checks="RequireThis"/>
    <suppress files=".*" checks="InnerTypeLastCheck"/>
    <suppress files="ConfigurationPrinter.*\.java" checks="SpringLeadingWhitespace"/>
    <suppress files="CustomConfigurationTests\.java" checks="SpringLeadingWhitespace"/>
    <suppress files="[\\/]generated[\\/]" checks="[a-zA-Z0-9]*"/>
</suppressions>
